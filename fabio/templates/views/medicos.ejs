<% include partials/header %>

      <!-- Start Section -->

      <section class=" position-relative set-bg breadcrumb-container" style="background-image: url(&quot;assets/images/breadcrumb-img1.jpg&quot;); background-position: center center; background-size: cover; background-repeat: no-repeat;">

        <div class="overlay op-P9"></div>

        <div class="container">

            <div class="row">

                <div class="col-md-12">

                    <div class="breadcrumb-inner align-items-start flex-column justify-content-center   d-flex ">

                        <h3 class="semi-bold text-uppercase text-white">Parceiros</h3>

                        <nav aria-label="breadcrumb">

                            <ol class="breadcrumb bg-transparent mb-0 pl-0 p-0">

                                <li class="breadcrumb-item active"><a href="/">Home</a></li>

                                <li class="breadcrumb-item " aria-current="page"><a href="/medicos">Parceiros</a></li>

                            </ol>

                        </nav>

                    </div>

                </div>

            </div>

        </div>

    </section>

    <!-- Start Section -->



    <!-- Start Section -->

    <section class="m-pt-140  m-bg-1 m-pb-140">

        <div class="container">

            <div class="row">
                <% medicos.forEach( function(medicos) {  %>
                    <div class="col-md-4 col-sm-6">

                        <div class="signle-doctor-slide position-relative mb-50">

                            <img src="<%=medicos.image.url%>" alt="">

                            <div class="doctors-info top-radius-pill bg-white text-center position-absolute pt-35 pb-35 full-width">

                                    <a href="<%=medicos.website%>" target="_blank" class="more-btn d-inline-flex  align-items-center justify-content-center bg-white rounded-circle position-absolute">

                                    <i class="fas fa-long-arrow-alt-right"></i>

                                </a>

                                <div class="social-icon position-absolute">

                                    <a href="<%=medicos.facebook%>"  target="_blank" class="text-active">

                                        <i class="fab fa-facebook-f"></i>

                                    </a>

                                    <a href="<%=medicos.twitter%>" target="_blank" class="text-active">

                                        <i class="fab fa-twitter"></i>

                                    </a>

                                    <a href="<%=medicos.instagram%>" target="_blank" class="text-active">

                                        <i class="fab fa-pinterest-p"></i>

                                    </a>

                                </div>

                                <h5 class="text-dark2 m-0 semi-bold"><%=medicos.title%></h5>

                                <p class=" m-0 fz-poppins"><%=medicos.speciality%></p>

                            </div>

                        </div> <!-- signle-doctor-slide -->    

                    </div>
                <% }) %>
            </div>
    <!--
            <div class="row">

                <div class="col-md-12 text-center">

                    <a href="#" class="btn fadeInDown-slide animated no-border bg-transparent medim-btn grad-bg--3 solid-btn  text-medium radius-pill text-active text-uppercase text-white">

                        load more

                    </a>

                </div>

            </div>
        -->


        </div>

    </section>

    <!-- End Section -->


<!-- Start footer -->
<footer class="m-pt-60">
        <div class="container text-center">
            <div class="row">
                <div class="text-light-dark">
                    <i class="icofont-location-pin mr-2 mt-2 text-light-dark"></i>
                    <div class="media-body">
                        <p class="m-0  fz-poppins">Rua Padre Almeida, 310</p>
                        <p class="m-0  fz-poppins">Cambuí, Campinas - Medicina 310</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="footer-bottom text-center mt-5 m-bg-1 pt-4 pb-4">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <p class="mb-1 mt-1">© 2019. Todos os direitos reservados.</p>
                    </div>
                </div>
                <a href="http://www.carinebitencourt.com.br" target="blank">
                    <div id="carinebitencourt">Studio de Marketing - Carine Bitencourt</div>
                </a>
            </div>
        </div>
    </footer>
    <!-- End footer -->

    <!-- Start scroll-top  Section -->
    <div class="scroll-top" style="display: none;">
        <i class="icofont-rounded-up"></i>
    </div>
    <!-- End  scroll-top Section -->


    <!-- js file -->
    <script src="js/jquery-3.3.1.js"></script>
    <!-- plugins js -->
    <script src="/js/plugins.js"></script>
    <!-- main js -->
    <script src="js/main.js"></script>


</body>
</html>